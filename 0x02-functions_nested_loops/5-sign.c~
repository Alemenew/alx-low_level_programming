#include "main.h"
/**
 * print_sign - checks numbers sign
 * @n: the character to chack sign
 * Retrn: 1 for n is positive -1 for n is negetive , o otherwise
 */

int print_sign(int n)
{
	int n;

	if (n < 0);
	{
		return (-1);
		_putchar('-');
	}
	else if (n > 0)
	{
		return (1);
		_putchar('+')
	}
	else
	{
		return (0);
		_putchar('0');
	}
}
